import java.util.*;

class Main{
    public static void main(String [] args){
        int max=5;
        Scanner sc = new Scanner(System.in);
        
        int[] arr = new int[max];
        int sum =0;
        System.out.println("enter 5 numbers");
        for(int i=0; i<5; i++){
            arr[i]=sc.nextInt();
            sum += arr[i];
        }
        System.out.println("sum:" + sum);
    }
}
